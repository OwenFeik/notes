{
    "latex-workshop.latex.recipes": [
        {
            "name": "default build",
            "tools": [
                "pdflatex"
            ]
        }
    ],

    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        }
    ]
}
